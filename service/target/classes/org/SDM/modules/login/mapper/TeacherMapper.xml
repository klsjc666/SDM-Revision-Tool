<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.SDM.modules.login.mapper.TeacherMapper">
    <select id="find" parameterType="org.SDM.modules.login.entity.Teacher" resultType="java.lang.Integer">
        select 1
        from teacher
        where TEACHER_USERNAME = #{username} and TEACHER_PASSWORD = #{password}
    </select>
</mapper>